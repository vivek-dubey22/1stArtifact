name: 1stArtifact

on: push

env
ARTIFACT_NAME: myartifact
jobs:
 main:
    runs-on: ubuntu-latest
    steps:
      -name:check out the code
        uses:actions/checkout@version2
        -name:upload the artifact
        uses:actions/upload-artifact@v2
        with
         name:${{ env.ARTIFACT_NAME }}
          path:.
